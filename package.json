{
	"name": "weather-app",
	"version": "1.0.0",
	"private": true,
	"homepage": "https://vorasudh.github.io/react-weather-app",
	"dependencies": {
		"@material-ui/core": "^4.9.14",
		"@material-ui/lab": "^4.0.0-alpha.53",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"lodash": "^4.17.15",
		"moment": "^2.26.0",
		"react": "^16.13.1",
