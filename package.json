	"scripts": {
		"start": "npm run format && npm run lint && react-scripts start",
		"build": "npm run format && npm run lint && react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint --ext .js,.jsx src --color --fix",
